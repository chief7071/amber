<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
<script type="text/javascript">
    //<![CDATA[

    if(typeof onlinebrand == "undefined") onlinebrand = {};

    onlinebrand.googlemap = function(){
      self = this;
      this.map;
      this.panorama;
      this.markers = [];
      this.infoWindow;
      this.directionsService = null;
      this.directionsDisplay = null;
      this.tmp_lat = null;
      this.url = '/googlemap/index/search';
      this.defaultLocation = 'Denmark';
      this.initDone = false;


      this._init = function(){
        var latlng = new google.maps.LatLng(56.21338352673757, 10.117876953125009);
				var mapStyle = [
    {
        "featureType": "water",
        "elementType": "geometry",
        "stylers": [
            {
                "color": "#e9e9e9"
            },
            {
                "lightness": 17
            }
        ]
    },
    {
        "featureType": "landscape",
        "elementType": "geometry",
        "stylers": [
            {
                "color": "#f5f5f5"
            },
            {
                "lightness": 20
            }
        ]
    },
    {
        "featureType": "road.highway",
        "elementType": "geometry.fill",
        "stylers": [
            {
                "color": "#ffffff"
            },
            {
                "lightness": 17
            }
        ]
    },
    {
        "featureType": "road.highway",
        "elementType": "geometry.stroke",
        "stylers": [
            {
                "color": "#ffffff"
            },
            {
                "lightness": 29
            },
            {
                "weight": 0.2
            }
        ]
    },
    {
        "featureType": "road.arterial",
        "elementType": "geometry",
        "stylers": [
            {
                "color": "#ffffff"
            },
            {
                "lightness": 18
            }
        ]
    },
    {
        "featureType": "road.local",
        "elementType": "geometry",
        "stylers": [
            {
                "color": "#ffffff"
            },
            {
                "lightness": 16
            }
        ]
    },
    {
        "featureType": "poi",
        "elementType": "geometry",
        "stylers": [
            {
                "color": "#f5f5f5"
            },
            {
                "lightness": 21
            }
        ]
    },
    {
        "featureType": "poi.park",
        "elementType": "geometry",
        "stylers": [
            {
                "color": "#dedede"
            },
            {
                "lightness": 21
            }
        ]
    },
    {
        "elementType": "labels.text.stroke",
        "stylers": [
            {
                "visibility": "on"
            },
            {
                "color": "#ffffff"
            },
            {
                "lightness": 16
            }
        ]
    },
    {
        "elementType": "labels.text.fill",
        "stylers": [
            {
                "saturation": 36
            },
            {
                "color": "#333333"
            },
            {
                "lightness": 40
            }
        ]
    },
    {
        "elementType": "labels.icon",
        "stylers": [
            {
                "visibility": "off"
            }
        ]
    },
    {
        "featureType": "transit",
        "elementType": "geometry",
        "stylers": [
            {
                "color": "#f2f2f2"
            },
            {
                "lightness": 19
            }
        ]
    },
    {
        "featureType": "administrative",
        "elementType": "geometry.fill",
        "stylers": [
            {
                "color": "#fefefe"
            },
            {
                "lightness": 20
            }
        ]
    },
    {
        "featureType": "administrative",
        "elementType": "geometry.stroke",
        "stylers": [
            {
                "color": "#fefefe"
            },
            {
                "lightness": 17
            },
            {
                "weight": 1.2
            }
        ]
    }
];


        var myOptions = {
            center: latlng,
												styles: mapStyle,
            mapTypeId: google.maps.MapTypeId.ROADMAP,
            disableDefaultUI: false,
            zoomControl: true
        }

        this.map = new google.maps.Map(document.getElementById("map_canvas"), myOptions);

        copyrightNode = document.createElement('div');
        copyrightNode.id = 'copyright-control';
        copyrightNode.style.fontSize = '11px';
        copyrightNode.style.color = '#000';
        copyrightNode.style.fontFamily = 'Arial, sans-serif';
        copyrightNode.style.margin = '0 0 2px 0';
        copyrightNode.style.whiteSpace = 'nowrap';
        copyrightNode.style.right = '0';
        copyrightNode.style.bottom = '17px';
        copyrightNode.index = 1;
        copyrightNode.innerHTML = "<a href='http://www.onlinebrand.dk' target='_blank' title='Webbureau onlineBRAND'>Made by onlineBRAND</a>";

        this.map.controls[google.maps.ControlPosition.BOTTOM_LEFT].push(copyrightNode);

        this.infoWindow = new google.maps.InfoWindow({maxWidth: 500});

        locationSelect = document.getElementById("locationSelect");
        locationSelect.onchange = function(){
            var markerNum = locationSelect.options[locationSelect.selectedIndex].value;
            if(markerNum != "none"){
                google.maps.event.trigger(markers[markerNum], 'click');
            }
        };

        this.directionsService = new google.maps.DirectionsService();
        this.directionsDisplay = new google.maps.DirectionsRenderer();

        this.geocode();
      }

      this.geocode = function(){
        var geocoder = new google.maps.Geocoder();

        geocoder.geocode({address: this.defaultLocation}, function(results, status){
            if(status == google.maps.GeocoderStatus.OK){
                self.getLocations(results[0].geometry.location, true);
            }
        });
      }

      this.searchLocations = function(){
          var country = document.getElementById("countrySelect").value;
          var address = document.getElementById("addressInput").value;
          var location = country + ' ' + address;

          this.stopRouteGuidance();

          var geocoder = new google.maps.Geocoder();

          geocoder.geocode({address: location}, function(results, status){
              if(status == google.maps.GeocoderStatus.OK){
                  self.getLocations(results[0].geometry.location);
              }
          });
      }

      this.clearLocations = function(){
        this.infoWindow.close();

        for(var i = 0; i < this.markers.length; i++){
            this.markers[i].setMap(null);
        }

        this.markers.length = 0;
        jQuery("#selectLocations").html("");
      }

      this.getLocations = function(center, init){
        this.clearLocations();
        var country = document.getElementById("countrySelect").value;
        var countryCode = jQuery("#countrySelect option:selected").attr('data-code');
        var address = document.getElementById("addressInput").value;
        var data = jQuery("#ob-googlemap-filter-search").serialize();
        var url = this.url;
        var _init = "false";

        if(init == true){
          var _init = "true";
        }

        $ob.messages.loading();

        try {
          jQuery.ajax({
            url: url,
            dataType: "json",
            type: "post",
            data: {address: address, countryCode: countryCode, lat: center.lat(), lon: center.lng(), init: _init},
            success: function(data){
              var bounds = new google.maps.LatLngBounds();
              var i = 0;

              jQuery.each(data, function(){
                var name = this.name
                var address = this.address;
                var latlng = new google.maps.LatLng(
                parseFloat(this.lat),
                parseFloat(this.lon));

               self.addLocationOption(i, this);
               self.addMapMarker(latlng, this);

                bounds.extend(latlng);
                i++;
              });

			jQuery(window).trigger('resize');

              self.map.fitBounds(bounds);


              if(i != 0){
                self.map.fitBounds(bounds);
                jQuery("#ob-googlemap-error").hide();
              } else {
                jQuery("#ob-googlemap-error").show();
                var geocoder = new google.maps.Geocoder();
                geocoder.geocode({'address': country}, function(results, status) {
                    if (status == google.maps.GeocoderStatus.OK) {
                        self.map.setCenter(results[0].geometry.location);
                    }
                });
              }

              if(address == "" && self.initDone == true){
                  var geocoder = new google.maps.Geocoder();
                  geocoder.geocode({'address': country}, function(results, status) {
                      if (status == google.maps.GeocoderStatus.OK) {
                          self.map.setCenter(results[0].geometry.location);
                          self.map.fitBounds(results[0].geometry.bounds);
                      }
                  });
              }

              locationSelect.style.visibility = "visible";
              locationSelect.onchange = function(){
                var markerNum = locationSelect.options[locationSelect.selectedIndex].value;
                google.maps.event.trigger(markers[markerNum], 'click');
              };

              self.initDone = true;

              $ob.messages.hide();
            },
            error: function(request, error){
              alert(error);
            }
          });

        } catch(e){}
      }

      this.addMapMarker = function(latlng, item){
        var html = "<div style='display:block;white-space:nowrap;' class='gmap-info-window'><p><b>" + item.name + "</b></p><p>" + item.address +"</p><p>"+item.zip+" "+item.city+"</p>";

        if(item.phone != ""){
            html += "<p>Tel. "+item.phone+"</p>";
        }
        if(item.website != ""){
          html += '<p><a target="_blank" href="http://'+item.website+'">'+item.website+'</a></p>';
        }
        if(item.mail != ""){
          html += '<p>'+item.mail+'</p>';
        }
        html += "<a class='ob-googlemap-routeguidance-link' href='javascript:void(0);' onclick='javascript:obgooglemap.setRouteDestination(\""+item.lat+"\", \""+item.lon+"\");'></a> <a href='javascript:void(0);' onclick='javascript:obgooglemap.setStreetView(\""+item.lat+"\", \""+item.lon+"\");'>Street view</a>";

        html += "</div>";

        var marker = new google.maps.Marker({
            map: self.map,
            position: latlng,
												icon: '/skin/frontend/default/onlinebrand/images/mapmarker.png'
        });

        google.maps.event.addListener(marker, 'click', function(){
            self.infoWindow.setContent('<div class="scrollFix" style="line-height: 1.35; overflow:hidden; white-space: nowrap">'+html+'</div>');
            self.infoWindow.open(self.map, marker);
        });

        this.markers.push(marker);
      }

      this.addLocationOption = function(num, item){
        var html = '<div onclick="javascript:obgooglemap.focusLocation(\''+item.address+'\', \''+num+'\');" class="col-lg-3 col-md-3 col-sm-3 col-xs-6 mapheight">'
                  + '<center><img class="eqheight" src="/media/'+item.image+'" /></center>'
                  + '<h4>'+item.name+'</h4>'
                  + '<p>'+item.address+'</p>'
                  + '<p>'+item.zip+' '+item.city+'</p>'
                  + '<p>'+item.open+'</p>'
                  + '<p>'+item.close+'</p>'



        if(item.phone != ""){
          html += '<p><i class="ob-icon-phone"></i>'+item.phone+'</p>';
        }
        if(item.website != ""){
          html += '<p><a target="_blank" href="http://'+item.website+'">'+item.website+'</a></p>';
        }
        if(item.mail != ""){
          html += '<p>'+item.mail+'</p>';
        }

        html += '</div>';

        jQuery("#locationSelect").show();
        jQuery("#selectLocations").prepend(html);
      }

      this.focusLocation = function(address, num){
        var geocoder = new google.maps.Geocoder();
        var latLng = this.markers[num].getPosition();

        this.map.setCenter(latLng);
        this.map.setZoom(14);
        google.maps.event.trigger(this.markers[num], 'click');
        jQuery('html, body').animate({scrollTop:100 + 'px'}, '0');
      }

      this.setRouteDestination = function(lat, lon){
        this.tmp_lat = new google.maps.LatLng(
                parseFloat(lat),
                parseFloat(lon));
        jQuery("#ob-googlemap-directions-container").fadeIn();
      }

      this.stopRouteGuidance = function(){
        jQuery("#ob-googlemap-directions-container").fadeOut();
        jQuery("#directions").hide();
        this.directionsDisplay.setMap(null);
      }

      this.startRouteGuidance = function(){
        this.directionsDisplay.setMap(this.map);
        this.directionsDisplay.setPanel(document.getElementById('directions'));

        jQuery("#directions").show();

        var request = {
            origin: jQuery("#from").val(),
            destination: this.tmp_lat,
            travelMode: google.maps.TravelMode.DRIVING
        };

        this.directionsService.route(request, function(result, status){
            if(status == google.maps.DirectionsStatus.OK){
                self.directionsDisplay.setDirections(result);
            } else {
                jQuery("#directions").text("Ruten kunne ikke beregnes.");
            }
        });
      }

      this.setStreetView = function(lat, lon){
        this.stopRouteGuidance();
        this.tmp_lat = new google.maps.LatLng(
                parseFloat(lat),
                parseFloat(lon));

        var panoramaOptions = {
          position: this.tmp_lat,
          pov: {
            heading: 34,
            pitch: 10
          },
          enableCloseButton: true
        };

        this.panorama = new google.maps.StreetViewPanorama(document.getElementById("map_canvas"), panoramaOptions);
        this.map.setStreetView(this.panorama);
      }

      return this._init();
    };


    //]]>
</script>



<div id="ob-googlemap-directions-container" style="height:500px;width: 340px;background:#fff;display:none;position:absolute;right:0;z-index:1000000;">
